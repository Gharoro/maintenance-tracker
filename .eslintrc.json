{
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "mocha": true
  },
  "extends": [
    "airbnb-base"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "no-console": 0,
    "eol-last": 0,
    "camelcase": 0,
    "comma-dangle": 0,
    "consistent-return": "off",
    "object-curly-newline": "off",
    "arrow-body-style": "off",
    "no-else-return": "off",
    "no-shadow": "off"
  }
}